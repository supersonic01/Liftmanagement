<Application x:Class="Liftmanagement.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Liftmanagement"
             xmlns:converters="clr-namespace:Liftmanagement.Converters"
             xmlns:common="clr-namespace:Liftmanagement.Common"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"></converters:InverseBooleanConverter>
        <converters:DateTimeConverter x:Key="DateTimeConverter"></converters:DateTimeConverter>
        <converters:DateFixerConverter x:Key="DateFixerConverter"></converters:DateFixerConverter>
        <converters:DateConverter x:Key="DateConverter"></converters:DateConverter>
        <converters:BooleanToImageConverter x:Key="BooleanToImageConverter"></converters:BooleanToImageConverter>
        <converters:MultiConverter x:Key="MultiConverter"></converters:MultiConverter>
        <converters:ValueToForegroundColorConverter x:Key="valueToForeground"></converters:ValueToForegroundColorConverter>
        <common:CustomLanguage x:Key="CustomLanguage"></common:CustomLanguage>

    </Application.Resources>
</Application>
